%start List
%title "A possibly empty comma separated list of integers"
%comment "A trailing comma is allowed."

%%

/* 0 */ List: ListOpt TrailingComma;
/* 1 */ ListOpt: Num ListOptList /* Vec */;
/* 2 */ ListOptList: "," Num ListOptList; // Vec<T>::Push
/* 3 */ ListOptList: ; // Vec<T>::New
/* 4 */ ListOpt: ;
/* 5 */ Num: "0|[1-9][0-9]*";
/* 6 */ TrailingComma: TrailingCommaOpt;
/* 7 */ TrailingCommaOpt: ",";
/* 8 */ TrailingCommaOpt: ;
